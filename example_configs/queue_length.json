{
    "report-interval": 3000,
    "metrics": [
        {
            "name": "crawler.requests.queue.length",
            "command": "zcard all:requests"
        },
        {
            "name": "crawler.crawled.queue.length",
            "command": "llen scraped:urls",
            "report-interval": 5000
        }
    ]
}
